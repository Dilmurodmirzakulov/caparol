
    $(function() {
        // Definiți funcția pentru evenimentul mouseenter
        function mouseEnterHandler() {
          $('.teaser2020--0 .fly-out').slideDown(500, 'swing');
        }
      
        // Definiți funcția pentru evenimentul mouseleave
        function mouseLeaveHandler() {
          $('.teaser2020--0 .fly-out').slideUp(500, 'swing');
        }
      
        // Atașați funcțiile la evenimentele mouseenter și mouseleave
        $('.teaser2020--0').mouseenter(mouseEnterHandler);
        $('.teaser2020--0').mouseleave(mouseLeaveHandler);
      
        // Funcția pentru prevenirea resetării
        function preventReset() {
          // Eliminați legăturile evenimentelor înainte de a le atașa din nou
          $('.teaser2020--0').off('mouseenter', mouseEnterHandler);
          $('.teaser2020--0').off('mouseleave', mouseLeaveHandler);
          
          // Alte acțiuni sau cod aici
      
          // Atașați funcțiile din nou la evenimentele mouseenter și mouseleave
          $('.teaser2020--0').mouseenter(mouseEnterHandler);
          $('.teaser2020--0').mouseleave(mouseLeaveHandler);
        }
      });
      


