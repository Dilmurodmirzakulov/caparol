        if (cookieNotificationLocationBottom && (!cookieDiscreetLink)) {
            $('div.cc-cookies').css("top", "auto");
            $('div.cc-cookies').css("bottom", "0")
        };
        if (cookieNotificationLocationBottom && cookieDiscreetLink && cookiePolicyPage) {
            $('div.cc-cookies').css("top", "auto");
            $('div.cc-cookies').css("bottom", "0")
        };
        $('.cc-cookie-accept, .cc-cookie-decline').click(function(e) {
            e.preventDefault();
            if ($(this).is('[href$=#decline]')) {
                $.cookie("cc_cookie_accept", null, {
                    path: '/'
                });
                $.cookie("cc_cookie_decline", "cc_cookie_decline", {
                    expires: cookieExpires,
                    path: '/'
                });
                if (options.cookieDomain) {
                    $.cookie("__utma", null, {
                        domain: '.' + options.cookieDomain,
                        path: '/'
                    });
                    $.cookie("__utmb", null, {
                        domain: '.' + options.cookieDomain,
                        path: '/'
                    });
                    $.cookie("__utmc", null, {
                        domain: '.' + options.cookieDomain,
                        path: '/'
                    });
                    $.cookie("__utmz", null, {
                        domain: '.' + options.cookieDomain,
                        path: '/'
                    })
                }
            } else {
                $.cookie("cc_cookie_decline", null, {
                    path: '/'
                });
                $.cookie("cc_cookie_accept", "cc_cookie_accept", {
                    expires: cookieExpires,
                    path: '/'
                })
            };
            $(".cc-cookies").fadeOut(function() {})
        });
        $('a.cc-cookie-reset').click(function(f) {
            f.preventDefault();
            $.cookie("cc_cookie_accept", null, {
                path: '/'
            });
            $.cookie("cc_cookie_decline", null, {
                path: '/'
            });
            $(".cc-cookies").fadeOut(function() {})
        });
        $('.cc-cookies-error a.cc-cookie-accept').click(function(g) {
            g.preventDefault();
            $.cookie("cc_cookie_accept", "cc_cookie_accept", {
                expires: cookieExpires,
                path: '/'
            });
            $.cookie("cc_cookie_decline", null, {
                path: '/'
            })
        })
    }
})(jQuery);
var wow, ieVersion = detectIE()

function triggerWowCallback() {
    if (typeof wow != 'undefined') {
        wow.scrolled = !0;
        wow.scrollCallback()
    }
}

function detectIE() {
    var ua = window.navigator.userAgent,
        msie = ua.indexOf('MSIE ');
    if (msie > 0) return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
    var trident = ua.indexOf('Trident/');
    if (trident > 0) {
        var rv = ua.indexOf('rv:');
        return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10)
    };
    var edge = ua.indexOf('Edge/');
    if (edge > 0) return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
    return false
}

function svgFallback() {
    if (Modernizr.svg == false || (ieVersion != false && ieVersion <= 10)) $('img[src$="svg"]').attr('src', function() {
        return $(this).attr('src').replace('.svg', '.png')
    })
}

function resizeIframe(obj) {
    obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px'
};
(function($) {
    $(function() {
        $(document).ready(function() {
            $('.button-wrapper #nav-icon').click(function() {
                $(this).toggleClass('open')
            });
            var isMobile = (/android|webos|iphone|ipod|blackberry|iemobile|opera mini/i.test(navigator.userAgent.toLowerCase()));
            svgFallback();
            if (ieVersion != false) $('html').addClass('ie-version-' + ieVersion);
            var $headerbannerfound = $('.tx-caparolvideorandom .header-unit').find('div');
            if ($headerbannerfound.hasClass('header-banner')) $('#topic-selection-start').addClass('header-banner-active');
            $('.nav-top li a').tooltip();
            var $tilesContainer = $('div[id*="tiles-container-"]');
            $("img").unveil(200, function() {
                $(this).load(function() {
                    if ($tilesContainer.length > 0 && $tilesContainer.hasClass('initialized') == true) $tilesContainer.packery()
                })
            });
            $(window).load(function() {
                $("html,body").trigger("scroll");
                $("html,body").trigger("resize")
            });
            $('#caparolTabs').tabCollapse();
            wow = new WOW();
            wow.init();
            $('.carousel').bcSwipe({
                threshold: 50
            });
            $('div[id*="accordion-"], div#caparolTabs-accordion').on('shown.bs.collapse', function(e) {
                var offset = $(this).find('.panel > .panel-collapse.in').offset();
                if (offset) {
                    var yPos = offset.top - 180;
                    $('html,body').velocity('finish').velocity('scroll', {
                        duration: 500,
                        offset: (yPos > 0) ? yPos : 0,
                        easing: 'ease-in-out',
                        complete: function() {
                            triggerWowCallback()
                        }
                    })
                } else triggerWowCallback()
            });
            $('.dropdown-award').hide();
            $('.dropdown-award-nav-arrow').hide();
            $(document).on('click', ".inspiration-award", function(e) {
                var offset = $('.inspiration-award').offset(),
                    positionNew = (parseInt(offset.left) + parseInt('48'));
                document.getElementById("dropdown-award-nav-arrow").style.left = positionNew + "px";
                $('#dropdown-award-nav-arrow').show('slow');
                $('.dropdown-award').slideDown('slow')
            });
            $(document).on('click', ".dropdown-award .close-button", function(e) {
                $('#dropdown-award-nav-arrow').hide();
                $('.dropdown-award').slideUp('slow')
            });
            $(window).resize(function() {
                if ($('.inspiration-award').offset()) {
                    var offset = $('.inspiration-award').offset(),
                        positionNew = (parseInt(offset.left) + parseInt('48'));
                    document.getElementById("dropdown-award-nav-arrow").style.left = positionNew + "px"
                }
            });
            $('#uebernachtunggroup').hide();
            if ($('#uebernachtung #uebernachtungja').is(':checked')) $("#uebernachtunggroup").show();
            if ($('.content-readmore').length) $('.content-readmore').each(function() {
                var $button = $(this),
                    $parent = $button.parent();
                $parent.append('<div class="temp-content-readmore-end"></div>');
                $button.nextUntil('.temp-content-readmore-end').wrapAll('<div class="readmore-text" />');
                $('.temp-content-readmore-end', $parent).remove();
                $button.appendTo($parent);
                var $readMoreText = $parent.find('.readmore-text'),
                    height = $readMoreText.height();
                $readMoreText.css('height', '0px');
                $button.click(function() {
                    $readMoreText.toggleClass('shown');
                    $button.remove();
                    if ($readMoreText.hasClass('shown')) {
                        $readMoreText.css('height', height + 'px')
                    } else $readMoreText.css('height', '0px')
                })
            })
        });
        $('body').on('hidden.bs.modal', '.modal', function() {
            $('video').trigger('pause')
        });
        $(window).load(function() {
            $(".se-pre-con").fadeOut("slow")
        });
        $(document).on('click', "#product-ti-link", function(e) {
            var productTabActive = $(this).attr('href');
            $('#caparolTabs li').removeClass('active');
            $(productTabActive + 'item').addClass('active')
        });
        $('#uebernachtung').on('click', 'input:radio', function(e) {
            if ($('#uebernachtungja').is(':checked')) {
                $("#uebernachtunggroup").show()
            } else $("#uebernachtunggroup").hide()
        });
        $(function() {
            function stripTrailingSlash(str) {
                if (str.substr(-1) == '/') return str.substr(0, str.length - 1);
                return str
            };
            var url = window.location.pathname,
                activePage = stripTrailingSlash(url);
            $('#caparolTabs li a').each(function() {
                var currentPage = stripTrailingSlash($(this).attr('href'));
                if (activePage == currentPage) $(this).parent().addClass('active')
            })
        })
    })
