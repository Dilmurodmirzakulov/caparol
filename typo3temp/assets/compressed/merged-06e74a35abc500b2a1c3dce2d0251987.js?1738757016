                d = this.options.isHorizontal ? "innerHeight" : "innerWidth";
            c && a[d] == this.size[d] || this.layout()
        }, i.prototype.itemDragStart = function(a) {
            this.stamp(a);
            var b = this.getItem(a);
            b && b.dragStart()
        }, i.prototype.itemDragMove = function(a, b, c) {
