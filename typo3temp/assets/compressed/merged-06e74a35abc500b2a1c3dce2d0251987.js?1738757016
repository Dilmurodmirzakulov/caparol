    var carouselContainer = jQuery('.hero .carousel');
    carouselContainer.find('.active').addClass('start')
});
$(window).load(function(){

if( !$('body').hasClass('page-5') ){

	if ($(".breadcrumbProduse").next().html().length > 0) {

		$('html, body').animate({
	      scrollTop: $(".breadcrumbProduse").next().offset().top
	    }, 1000)
	}

	else if ($(".teaserMenuLevel2").next().html().length > 0) {
		$('html, body').animate({
	      scrollTop: $(".teaserMenuLevel2").next().offset().top
	    }, 1000)
	}

	else if ($(".teaserMenuLevel2").html().length > 0) {
		$('html, body').animate({
	      scrollTop: $(".teaserMenuLevel2").offset().top
	    }, 1000)
	}


	// else {
	// 	$('html, body').animate({
	//       scrollTop: $(".productsBox").offset().top
	//     }, 1000)
	// }
}

});

$(document).ready(function() {
    $('section.main .content').addClass('selectContainer');

    $('.selectContainer .frame-type-textpic .gallery-row').parent().addClass('detailProductContent');
    $('.selectContainer .detailProductContent .gallery-item.col-xs-12').removeClass('col-xs-12').addClass('col-sm-12 col-md-4');
    $('.selectContainer .detailProductContent > p').prependTo('.selectContainer .detailProductContent .gallery-row');
    $('.selectContainer .detailProductContent > strong, .selectContainer .detailProductContent > a').wrapInner( "<p></p>").prependTo('.selectContainer .detailProductContent .gallery-row');


    $('.selectContainer .detailProductContent .gallery-row').wrapInner( "<div class='col-md-5'></div>");
    $('.selectContainer .detailProductContent .gallery-item').appendTo('.selectContainer .detailProductContent .gallery-row');

    var imgs = $('.frame-type-textpic .gallery-row').siblings('img');
    imgs.each(function(){
        $(this).appendTo('.col-md-5');
    })

    // Dezactivare buton de download de la Produse
    // $('.frame-inner.detailProductContent .col-md-5 ').append('<div class="product-backlink"> <a href="#" id="product-ti-link" class="btn btn-default wow fadeInUp text-center hidden-xs" data-toggle="tab" style="visibility: visible; animation-name: fadeInUp;">Download</a> </div>');

    $('body').on('click', '.product-backlink', function(e){
    	e.preventDefault();

    	$('a.tx-tab-bar-item').each(function(){
    		var strr = $(this).html();
    		strr = strr.replace(/\s/g, '').toLowerCase();

    		if(strr == 'download') {
    			$( $(this) ).trigger('click');
    		}
    	});
    });




    $("p").each(function(){

	  if ($.trim($(this).text()) == "" && $.trim($(this).html()) == ""){
	    $(this).remove();
	  }
	});




    $('.tx-tab-panel .frame-container').each(function(){
    	if( $(this).find('iframe').length > 0 ) {
    		$(this).addClass('iframe-content');
    	}
    });

    var winWidth = $(window).width();

    if(winWidth < 760) {

        if($('ul.tx-tab-bar').length > 0) {
            var elemtToScroll = $('ul.tx-tab-bar').offset().top;
        }

        // var hh = elemtToScroll.height();

        // console.log(hh);

        // $('a.tx-tab-bar-item').click(function(){
        //     var ee = $(this).offset().top;

        //     setTimeout(function(){
        //         $('html, body').animate({
        //             // scrollTop: elemtToScroll.offset().top
        //             scrollTop: '+='+(elemtToScroll+ee)+'px'
        //         }, 800);
        //     }, 500);

        // });
    }




});

