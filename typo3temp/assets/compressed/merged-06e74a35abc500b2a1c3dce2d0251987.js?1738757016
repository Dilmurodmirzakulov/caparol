jQuery(document).ready(function() {
    var $container = jQuery('div[id*="tiles-container-"]');
    $container.prepend(jQuery('<div>').addClass('gutter-sizer')).prepend(jQuery('<div>').addClass('grid-sizer'));
    $container.imagesLoaded(function() {
        $container.packery({
            itemSelector: 'div[id*="tile-"]',
            columnWidth: '.grid-sizer',
            gutter: '.gutter-sizer',
            percentPosition: true
        })
    });
    $container.one('layoutComplete', function() {
        $container.find('div[id*="tile-"]').each(function(i, element) {
            jQuery(element).velocity({
                opacity: 1
            }, {
                delay: 200 * i,
                duration: 400,
                queue: false
            })
        });
        window.setTimeout(function() {
            $container.packery();
            $container.addClass('initialized')
        }, 1e3)
    })
});
